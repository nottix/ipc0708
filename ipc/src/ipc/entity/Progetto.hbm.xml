<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="ipc.entity">
  <class name="Progetto">
     <id name="id" column="ID">
        <generator class="native"/>
     </id>
     <property name="titolo"/>
     <property name="dataConsegna" type="timestamp" column="DATA_CONSEGNA"/>
     <property name="maxUploadPerStudente" type="integer" column="MAX_UPLOAD_CONSENTITI"/>
     <property name="maxDimGruppo" type="integer" column="MAX_DIM_GRUPPO"/>
     <property name="status"/>
     
     <property name="idCorso" column="ID_CORSO"/>
   </class>
</hibernate-mapping>