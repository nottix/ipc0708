<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="ipc.entity">
   <class name="Esame">
      <id name="id" column="ID">
         <generator class="native"/>
      </id>
      <property name="dataInizioPeriodoPrenotazione" type="timestamp" column="INIZIO_PRENOTAZIONE"/>
      <property name="dataFinePeriodoPrenotazione" type="timestamp" column="FINE_PRENOTAZIONE"/>
   	  <property name="status"/>
   </class>
</hibernate-mapping>