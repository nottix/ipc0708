<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="ipc.entity">
   <class name="IscrizioneCorso">
      <id name="id" column="ID">
         <generator class="native"/>
      </id>
      <property name="flag"/>
      <property name="dataIscrizione" type="timestamp" column="DATA_ISCRIZIONE"/>
      <property name="status"/>
      
      <property name="idStudente" column="EMAIL" />
  	  <property name="idCorso" column="ID_CORSO" />
   </class>
</hibernate-mapping>