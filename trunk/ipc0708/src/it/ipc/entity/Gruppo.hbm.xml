<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="it.ipc.entity">
   <class name="Gruppo">
      <id name="id" column="ID">
         <generator class="native"/>
      </id>
      <property name="maxStudenti" type="integer" column="MAX_STUDENTI"/>
      <property name="minStudenti" type="integer" column="MIN_STUDENTI"/>
      <property name="status"/>
      <set name="listaStudenti" table="GRUPPO_STUDENTI">
         <key column="ID"/>
         <many-to-many column="EMAIL" class="it.ipc.entity.Account"/>
      </set>
   </class>
</hibernate-mapping>